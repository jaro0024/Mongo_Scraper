<div class="row">
    <div class="col-md-1">

    </div>
    <div class="col-md-10">
        <h1>Saved Articles</h1>
        {{#each allArticles}}
        <br>
        <div class="input-group">
            <a href="{{this.link}}" target="_blank" class="form-control" id="articleTitle">{{this.title}}</a>
            <span class="input-group-btn">
                <form method="POST" action="/delete/{{this.id}}?_method=PUT">
                    <button type="submit" class="btn btn-secondary">Delete Article</button>
                </form>
            </span>
            <span class="input-group-btn">
                <form method="GET" action="/articles/{{this.id}}">
                    <button type="submit" class="btn btn-secondary">Note</button>
                </form>
            </span>
        </div>
        {{/each}}
    </div>
    <div class="col-md-1">

    </div>
</div>




